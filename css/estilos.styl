@import url("fonts.css")
navi=#233142
textos=#e3e3e3
neg=#000
act=#455d7a
osc=#777
noet = #fff
titu=Quattrocento
plano=Fanwood
subs=Fanwood, Quattrocento
mor()
  background linear-gradient(lightgray, white)
  border-radius 15px
  padding 15px 60px
  display inline-block
  font-family arial
  font-weight bold
  color#7f7f7f
  text-decoration none
  text-shadow0px 1px 0px gray
  border1px solid #a7a7a7
  margin0px auto
  box-shadow 0px 2px 1px white inset, 0px -2px 8px white, 0px 2px 5px rgba(0, 0, 0, 0.1), 0px 8px 10px rgba(0, 0, 0, 0.1)
  -webkit-transitionbox-shadow 1s
port()
  background-attachment fixed
  background-position center center
  background-size cover
  height 307px
  text-align center
titslid()
  background rgba(0,0,0,0.6)
  color: #e0dfdc
  font-family titu
  font-size 3em
  font-weight normal
  letter-spacing: .1em
  margin 0
  padding 2em 0
  text-shadow 0 0 10px neg
  text-shadow: 0 -1px 0 #fff, 0 1px 0 #2e2e2e, 0 2px 0 #2c2c2c, 0 3px 0 #2a2a2a, 0 4px 0 #282828, 0 5px 0 #262626, 0 6px 0 #242424, 0 7px 0 #222, 0 8px 0 #202020, 0 9px 0 #1e1e1e, 0 10px 0 #1c1c1c, 0 11px 0 #1a1a1a, 0 12px 0 #181818, 0 13px 0 #161616, 0 14px 0 #141414, 0 15px 0 #121212, 0 22px 30px rgba(0, 0, 0, 0.9)
tits()
    background-color #e7e5e4
    font-size 2em
    letter-spacing .15em
    text-shadow 1px -1px 0 #767676, -1px 2px 1px #737272, -2px 4px 1px #767474, -3px 6px 1px #787777, -4px 8px 1px #7b7a7a, -5px 10px 1px #7f7d7d, -6px 12px 1px #828181, -7px 14px 1px #868585, -8px 16px 1px #8b8a89, -9px 18px 1px #8f8e8d, -10px 20px 1px #949392, -11px 22px 1px #999897, -12px 24px 1px #9e9c9c, -13px 26px 1px #a3a1a1, -14px 28px 1px #a8a6a6, -15px 30px 1px #adabab, -16px 32px 1px #b2b1b0, -17px 34px 1px #b7b6b5, -18px 36px 1px #bcbbba, -19px 38px 1px #c1bfbf, -20px 40px 1px #c6c4c4, -21px 42px 1px #cbc9c8, -22px 44px 1px #cfcdcd, -23px 46px 1px #d4d2d1, -24px 48px 1px #d8d6d5, -25px 50px 1px #dbdad9, -26px 52px 1px #dfdddc, -27px 54px 1px #e2e0df, -28px 56px 1px #e4e3e2
subs()
  color: #2c2c2c
  font-size 1.8em
  font-family titu
  font-weight bold
  letter-spacing: .05em
  text-shadow: 4px 4px 0px #d5d5d5, 7px 7px 0px rgba(0, 0, 0, 0.2)
body
  font-family titu,plano
  font-size 20px
  margin 0
  padding 0
  header 
    nav
      background navi
      color textos
      font-size 1em
      position fixed
      text-align right
      width 100%
      z-index 100
      a
        -webkit-transition all .3s ease-in
        color inherit
        display inline-block
        padding 2em 1em .5em
        text-decoration none
        transition all .3s ease-in
      img
        left 9px
        position absolute
        top 5%
        transition all .5s linear
        width 10%;
        z-index 1000
      span
        font-weight bold
        left 40px
        position absolute
        top 10px
        transition all .5s ease-in
    .menu_bar
      display none
footer.info
  background navi
  color textos
  padding 0 auto
  text-align center
  div 
    a
      color #bff4ed
      text-decoration none
      &:hover
        text-shadow 2px 2px 2px osc
  .social
    a
      color textos
      font-size 1.2em
      text-decoration none
iframe
  border 2 px solid osc
  width 90%
p
  text-align justify
#portion2
  port()
  background-image url("../images/ISC.jpg")
#portion3
  port()
  background-image url("../images/IIND.jpg")
#portion4
  port()
  background-image url("../images/IIAS.jpg")
#portion5
  port()
  background-image url("../images/IGE.jpg")
.small
  padding .5em 1em
#toTop 
  img
    width 50px 
    &:hover
      cursor pointer
#seccion_uno
  background-color noet
  font-family titu
  height 600px
  margin-top 90px
  .note
    background-color noet
    border 1px solid #808080
    border-radius 5px
    float left
    height 500px
    margin-left 75px
    top 30px
    width 25%
    .tittle_note
      subs()
      text-align center
  .texto_nota
    font-family plano
    padding 10px
    text-align justify
    text-align left
.active
  background act
.button
  text-align center
.button 
  .more
    mor()
    &:hover
      background linear-gradient(white, lightgray)
.img
  border 2px solid osc
  border-radius 15px
  width 80%
.imgs
  border 2px solid osc
  border-radius 15px
  width 20%
.info
  background textos
  color navi
  padding 3em 20%
  h3
    font-family titu
    color #131313
    tits()
  h4
    subs()
  h5
    font-size 1.2em
    font-weight bold
    font-family subs
  p
    font-family plano
    font-size 1.18em
  form
    border 2px solid osc
    width 1000px
    margin-left -10%
    table
      width 100%
      tr
        td
          padding-left 5%
          text-align center
          font-size 1.3em
          input
            width 80%
          textarea
            width 80%
  p
    font-family plano
.move
  top -60px
.movetext
  top -30px
.portion
  port()
  background-image url("../images/IINF.jpg")
  h2
    titslid()